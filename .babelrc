{
  "presets": [
    "es2015",
    "stage-1"
  ],
  "plugins": [
    [ "transform-react-jsx", { "pragma": "h" } ],
    [
      "module-resolver",
      {
        "root": ["./node_modules"],
        "alias": {
          "react": "preact-compat",
          "react-dom": "preact-compat"
        }
      }
    ]
  ]
}
